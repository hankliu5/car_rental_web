  <div>
    <%= link_to("New Car", new_cars_path) %>
  </div>
  <table>
    <thead>
      <tr>
        <td>#</td>
        <td>Plate</td>
        <td>Make</td>
				<td>Rate</td>
				<td>Style</td>
				<td>Location</td>
				<td>Checkout</td>
      </tr>
    </thead>
    <tbody>
      <% @cars.each do |car| %>
      <tr>
        <td>#</td>
        <td><%= link_to(truncate(car.plate, length: 20), group_path(car)) %></td>
				<td><%= car.make %></td>
				<td><%= car.model %></td>
				<td><%= car.rate %></td>
				<td><%= car.style %></td>
				<td><%= car.location %></td>
				<td><%= car.checkout %></td>
        <td>
          <%= link_to("Edit", edit_car_path(car)) %>
          <%= link_to("Delete", car_path(car), method: :delete, data: { confirm: "Are you sure?"} ) %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
